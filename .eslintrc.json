{
    "extends": [
        "airbnb",
        "prettier",
        "plugin:prettier/recommended",
        "prettier/react"
    ],
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "parser": "babel-eslint",
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [".js"]
            }
        ]
    }
}

