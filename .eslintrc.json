{
    "extends": [
        "airbnb",
        "prettier",
        "plugin:prettier/recommended",
        "prettier/react"
    ],
    "rules": {
        "jsx-a11y/label-has-for": [2, {
            "required": {
                "every": ["id"]
            }
        }]
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest": true
    },
    "parser": "babel-eslint"

}
